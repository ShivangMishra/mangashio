{
  "expo": {
    "androidStatusBar": {
      "backgroundColor": "#000000",
      "translucent": false,
      "hidden": false
    },
    "name": "mangashio",
    "slug": "mangashio",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mangashio"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "2366c5c3-dc21-4a68-bc55-7c56ca0ae061"
      }
    },
    "plugins": [
      "expo-router"
    ]
  }
}
